<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
        <entity name="Appto\ShoppingCart\Domain\ProductLine" table="sc_product_line" >
                <embedded name="name" class="Appto\ShoppingCart\Domain\ProductName" use-column-prefix="false"/>
                <embedded name="productPrice" class="Appto\ShoppingCart\Domain\ProductPrice" use-column-prefix="false"/>
                <embedded name="units" class="Appto\ShoppingCart\Domain\Units" use-column-prefix="false"/>

                <id name="entityId" type="integer" >
                        <generator strategy="AUTO"/>
                </id>
        </entity>
</doctrine-mapping>
